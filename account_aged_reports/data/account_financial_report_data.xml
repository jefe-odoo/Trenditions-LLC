<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="action_account_report_ar_inv_date" model="ir.actions.client">
        <field name="name">Aged Receivable by Invoice Date</field>
        <field name="tag">account_report</field>
        <field name="context" eval="{'model': 'account.aged.receivable.date'}" />
    </record>
    <record id="action_account_report_ap_inv_date" model="ir.actions.client">
        <field name="name">Aged Payable by Bill Date</field>
        <field name="tag">account_report</field>
        <field name="context" eval="{'model': 'account.aged.payable.date'}" />
    </record>
    <record id="account_reports.action_account_report_ar" model="ir.actions.client">
        <field name="name">Aged Receivable By Due Date</field>
    </record>
    <record id="account_reports.action_account_report_ap" model="ir.actions.client">
        <field name="name">Aged Payable By Due Date</field>
    </record>

    <!-- Account Financial Report Menuitems -->
    <menuitem id="menu_action_account_report_aged_receivable_inv_date" name="Aged Receivable By Invoice Date" action="action_account_report_ar_inv_date" parent="account_reports.account_reports_partners_reports_menu"/>
    <menuitem id="menu_action_account_report_aged_payable_inv_date" name="Aged Payable By Bill Date" action="action_account_report_ap_inv_date" parent="account_reports.account_reports_partners_reports_menu"/>

    <record model="ir.ui.menu" id="account_reports.menu_action_account_report_aged_receivable">
        <field name="name">Aged Receivable By Due Date</field>
    </record>
    <record model="ir.ui.menu" id="account_reports.menu_action_account_report_aged_payable">
        <field name="name">Aged Payable By Due Date</field>
    </record>

</odoo>